{
    "name": "xenon",
    "displayName": "Xenon",
    "description": "Xenon Language Support",
    "publisher": "Xenon-Lang",
    "icon": "build/XenonLangLogo.png",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/xenon-lang/vscode-support.git"
    },
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xenon",
            "aliases": ["Xenon", "xenon"],
            "extensions": [".x"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xenon",
            "scopeName": "source.x",
            "path": "./syntaxes/xenon.tmLanguage.json"
        }],
        "snippets": [
            {
              "language": "xenon",
              "path": "./snippets.json"
            }
        ]
    }
}